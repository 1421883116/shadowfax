{
    "name": "huang-yi/laravel-swoole-http",
    "description": "High performance HTTP server based on Swoole. Speeds up your Laravel and Lumen applications.",
    "keywords": ["swoole", "laravel", "lumen", "http", "server", "performance"],
    "license": "MIT",
    "authors": [
        {
            "name": "Huang Yi",
            "email": "coodeer@163.com"
        }
    ],
    "require": {
        "illuminate/console": "~5.2",
        "illuminate/contracts": "~5.2",
        "illuminate/http": "~5.2",
        "illuminate/pipeline": "~5.2",
        "illuminate/redis": "~5.2",
        "illuminate/routing": "~5.2",
        "illuminate/support": "~5.2"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "psr-4": {
            "HuangYi\\Http\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HuangYi\\Http\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "HuangYi\\Http\\HttpServiceProvider"
            ],
            "aliases": {
                "SwooleServer": "HuangYi\\Http\\Facades\\Server",
                "SwooleTable": "HuangYi\\Http\\Facades\\Table",
                "WebsocketNamespace": "HuangYi\\Http\\Facades\\WebsocketNamespace",
                "WebsocketRoute": "HuangYi\\Http\\Facades\\WebsocketRoute",
                "MessageRoute": "HuangYi\\Http\\Facades\\MessageRoute"
            }
        }
    }
}
