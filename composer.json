{
    "name": "huang-yi/shadowfax",
    "description": "Run Laravel on Swoole.",
    "keywords": ["laravel", "swoole", "http-server"],
    "license": "MIT",
    "authors": [
        {
            "name": "Huang Yi",
            "email": "coodeer@163.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "illuminate/container": "^6.0",
        "illuminate/http": "^6.0",
        "illuminate/support": "^6.0",
        "psr/container": "^1.0",
        "symfony/console": "^4.3.4"
    },
    "require-dev": {
        "laravel/laravel": "^6.0",
        "laravel/lumen": "^6.0",
        "phpunit/phpunit": "^8.5"
    },
    "autoload": {
        "psr-4": {
            "HuangYi\\Shadowfax\\": "src"
        },
        "files": [
            "src/Support/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "HuangYi\\Shadowfax\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "HuangYi\\Shadowfax\\ShadowfaxServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
