{
    "name": "huang-yi/shadowfax",
    "description": "Run Laravel on Swoole.",
    "keywords": ["laravel", "swoole", "coroutine", "http-server", "shadowfax"],
    "license": "MIT",
    "authors": [
        {
            "name": "Huang Yi",
            "email": "coodeer@163.com"
        }
    ],
    "support": {
        "source": "https://github.com/huang-yi/shadowfax",
        "issues": "https://github.com/huang-yi/shadowfax/issues"
    },
    "require": {
        "php": "^7.2",
        "ext-swoole": "^4.3.2",
        "huang-yi/swoole-watcher": "^1.0",
        "illuminate/container": "^6.0",
        "illuminate/http": "^6.0",
        "illuminate/support": "^6.0",
        "symfony/console": "^4.3.4"
    },
    "require-dev": {
        "huang-yi/swooleunit": "^1.0",
        "laravel/framework": "^6.0",
        "laravel/lumen-framework": "^6.0",
        "mockery/mockery": "^1.3",
        "phpunit/phpunit": "^8.5"
    },
    "autoload": {
        "psr-4": {
            "HuangYi\\Shadowfax\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "HuangYi\\Shadowfax\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "HuangYi\\Shadowfax\\ShadowfaxServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "bin": [
        "shadowfax"
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "test": "swooleunit"
    }
}
