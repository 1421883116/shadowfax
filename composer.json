{
    "name": "huang-yi/laravel-swoole-http",
    "description": "High performance HTTP server based on Swoole. Speeds up your Laravel and Lumen applications.",
    "keywords": ["swoole", "laravel", "lumen", "http", "server", "performance"],
    "license": "MIT",
    "authors": [
        {
            "name": "Huang Yi",
            "email": "coodeer@163.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://gitee.com/huang-yi/laravel-swoole-foundation.git"
        }
    ],
    "require": {
        "huang-yi/laravel-swoole-foundation": "dev-master",
        "illuminate/console": "~5.6",
        "illuminate/contracts": "~5.6",
        "illuminate/http": "~5.6",
        "illuminate/support": "~5.6"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "psr-4": {
            "HuangYi\\Swoole\\Http\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HuangYi\\Swoole\\Http\\Tests\\": "tests"
        }
    }
}
